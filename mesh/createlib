#! /bin/csh

cp -f mesh.h ~/Codes/include
cp -f utilities.h ~/Codes/include
cp -f quadtree.h ~/Codes/include

cd build/intermediates/mblock.build/Objects/ppc/
libtool -o libmesh.a coarsen.o collapse.o copy.o findmatch.o in_mesh.o insert.o out_mesh.o rebay.o setup.o swap.o tprims.o triangulate.o yaber.o mvpttobdry.o
mv -f libmesh.a ~/Codes/lib
cp -f quad.o ~/Codes/lib
cp -f xmalloc.o ~/Codes/lib

