# Name of file for output: 
logfile: generate
# Number of mesh blocks for each processor: 
nblock: 2
# flag for mesh adaptation: 
adapt: 1
# Input mesh filetype
# easymesh=0, gambit=1, tecplot=2, grid=3, text=4, binary=5, boundary=8: 
b0_filetype: 8
# Factor to increase memory allocation for mesh adaptation: 
b0_growth factor: 8000.0
# Name of mesh without extension: 
b0_mesh: ./timleft
# Input mesh filetype
# easymesh=0, gambit=1, tecplot=2, grid=3, text=4, binary=5, boundary=8: 
# Side type specifiers: 
b0_s4_type: comm
b0_s5_type: circle
b0_s5_center: 0.0 0.0
b0_s5_radius: 0.5
b1_mesh: ./timright
b1_growth factor: 8000.0
b1_filetype: 8
b1_s4_type: comm
