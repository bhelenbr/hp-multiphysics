#polynomial order: 
log2p: 2
#petsc command line args: 
petsc: -ksp_type preonly -pc_type lu -pc_factor_mat_solver_type mumps
#Number of iterative cycles: 
ncycle: 30
#restart: 0
#Iterative tolerances: 
absolute_tolerance: 1.0e-10
relative_tolerance: 5.0e-8
#Number of time steps: 
ntstep: 1
rsdl_debug: 0
jac_debug: 0
debug_output: 0
#Inverse time step: 
dtinv: cu*20
# Name of file for output: 
logfile: output
# Number of mesh blocks for each processor: 
nblock: 1 1
# flag for mesh adaptation: 
adapt: 1
# Factor to increase memory allocation for mesh adaptation: 
growth factor: 1000
#Physical properties: 
blocktype: cns
mesh_movement: coupled_deformable
mu: 0
prandtl: 1
# Initial and boundary condition function: 
gamma: 1.4
xs: 7.0
ys: 1.189014223226559
A: 1.384534025668615
B: 0.200869841251668
C: 1.115465974331382
D: 0.559366372602671
area: A-C*exp(-B*x0)*cos(D*x0-_pi)
darea: - B*C*exp(-B*x0)*cos(D*x0) - C*D*exp(-B*x0)*sin(D*x0)
# IC Upstream
Po: 1
RTo: 1/gamma
R: 1/gamma
b0_ibc: nozzle
b0_ibc_Aratio: area
b0_ibc_dArdx: darea
b0_ibc_RTo: RTo
b0_ibc_Po: Po
b0_ibc_xthroat: 5.0
# Name of mesh: 
b0_mesh: rstrt1_b0.grd
# Side type specifiers: 
xs_mesh: 7.0
b0_s1_hp_type: euler
b0_s1_type: symbolic
b0_s1_h: -area-x1
b0_s1_dhdx0: -darea
b0_s1_dhdx1: -1
b0_s1_curved: 1
b0_s1_r_dir: curved
b0_s2_type: coupled_symbolic_comm
b0_s2_h: x0-xs_mesh
b0_s2_dhdx0: 1
b0_s2_dhdx1: 0
b0_s2_hp_type: shock
b0_s2_c0_indices: 
b0_s3_hp_type: euler
b0_s3_type: symbolic
b0_s3_h: area-x1
b0_s3_dhdx0: darea
b0_s3_dhdx1: -1
b0_s3_curved: 1
b0_s3_r_dir: curved
b0_s4_hp_type: inflow
# IC downstream
M: 1.51867
pu: Po/((1+0.5*(gamma-1)*M^2)^(gamma/(gamma-1)))
RTu: RTo/(1+0.5*(gamma-1)*M^2)
rhou: pu/RTu
cu: sqrt(gamma*RTu)
pd: pu*(1.0+((2.0*gamma)/(gamma+1.0))*(M^2-1.0))
rhod: rhou*(((gamma+1.0)*M^2)/((gamma-1.0)*M^2+2.0))
ud: (rhou/rhod)*M*cu
RTd: pd/rhod
cd: sqrt(gamma*RTd)
Md: ud/cd
Astar: ys*Md/(((2+(gamma-1)*Md^2)/(gamma+1))^((gamma+1)/(2*(gamma-1))))
b1_ibc: nozzle
b1_ibc_Aratio: area/Astar
b1_ibc_dArdx: darea/Astar
b1_ibc_RTo: RTo
b1_ibc_Po: pd*(1+0.5*(gamma-1)*Md^2)^(gamma/(gamma-1))
b1_ibc_xthroat: 12
# Name of mesh: 
b1_mesh: rstrt1_b1.grd
# Side type specifiers: 
b1_s5_hp_type: euler
b1_s5_type: symbolic
b1_s5_h: -area-x1
b1_s5_dhdx0: -darea
b1_s5_dhdx1: -1
b1_s5_curved: 1
b1_s5_r_dir: curved
b1_s6_hp_type: outflow
b1_s7_hp_type: euler
b1_s7_type: symbolic
b1_s7_h: area-x1
b1_s7_dhdx0: darea
b1_s7_dhdx1: -1
b1_s7_curved: 1
b1_s7_r_dir: curved
b1_s2_type: coupled_symbolic_comm
b1_s2_h: x0-xs_mesh
b1_s2_dhdx0: 1
b1_s2_dhdx1: 0
b1_s2_hp_type: shock
b1_s2_c0_indices: 
b0_v1_hp_type: hp_deformable_free_pnt
b0_v2_hp_type: hp_deformable_free_pnt
b1_v1_hp_type: hp_deformable_free_pnt
b1_v2_hp_type: hp_deformable_free_pnt
b0_v1_wall_type: curved
b0_v2_wall_type: curved
b1_v1_wall_type: curved
b1_v2_wall_type: curved