ntstep: 1
log2p: 2
adapt: 1
error_estimator: energy_norm
error_target: 1.000e-04
logfile: generate
growth factor: 100.0
petsc: -ksp_type preonly -pc_type lu -pc_factor_mat_solver_type mumps
nu: 1.0
ax: 0.0
ay: 0.0
theta0: 3*_pi/4
lambda: _pi/(2*theta0)
ibc: symbolic
#ibc0: r^lambda*sin(lambda*theta)
ibc0: x1^lambda*sin(-lambda*x0/b0_theta_length)
key: value
b0_mesh: sector.d
b0_type: cd
b0_metric: mapped
b0_mapping: polar
b0_pnt: 0.0 0.0
b0_theta_length: 1.0
b0_s1_hp_type: dirichlet
b0_s2_hp_type: dirichlet
b0_s3_hp_type: adiabatic
b0_s4_hp_type: dirichlet
helper: l2error
