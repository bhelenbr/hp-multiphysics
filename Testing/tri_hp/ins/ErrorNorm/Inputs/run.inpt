log2p: 2
extra_finest_levels: 2
ngrid: 2
ncycle: 100
vwcycle: 2
itercrsn: 2
iterrfne: 2
ntstep: 10
dtinv: 0.0
adapt: 1
error_target: 0.1/(2^(nerror*(2^log2p)))
error_estimator: energy_norm
nerror: 0
curvature_sensitivity: 0.0
tolerance: 1.5
adapt_output: 1
output_interval: 1
absolute_tolerance: 1.0e-10
ibc: symbolic
re: 1.0/b0_mu
lda: .5*re - sqrt(re^2/4+ 4*_pi^2)
ibc0: 1.0 - cos(2*_pi*x1)*exp(lda*x0)
ibc1: lda/(2*_pi)*sin(2*_pi*x1)*exp(lda*x0)
ibc2: -.5*exp(2.*lda*x0)
b0_type: ins
b0_filetype: 3
b0_growth factor: 10000.0
b0_mesh: ../../grids/SQUARE/INOUT/square8
helper: l2error
b0_rho: 1.0
b0_mu: 0.025
b0_s4_type: symbolic
b0_s4_curved: 1
A: 0.0
b0_s4_h: x0-A*sin(2*_pi*x1)
b0_s4_dhdx1: -A*_pi*cos(2*_pi*x1)
b0_s4_dhdx0: 1.0
b0_s4_hp_type: inflow
b0_s2_hp_type: applied_stress
b0_s2_stress0: 2*b0_mu*(-lda*cos(2*_pi*x1)*exp(lda*x0))
b0_s2_stress1: b0_mu*(sin(2*_pi*x1)*exp(lda*x0)*(2*_pi+lda^2/(2*_pi)))
b0_s1_type: prdc
b0_s1_dir: 1
_s4_type: symbolic
_s4_curved: 1
_s4_h: x0-A*sin(2*_pi*x1)
_s4_dhdx1: -A*_pi*cos(2*_pi*x1)
_s4_dhdx0: 1.0
